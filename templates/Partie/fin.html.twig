{% extends 'base.html.twig' %}
{% block body %}
<section class="main-contenu">
 <div class="logo">
  <div class="image">
   <img src="img/logo.svg" alt="logo A la conquête du Graal">
  </div>
  <div class="titre">
   <h1>
    Partie terminée
   </h1>
  </div>
 </div>
</section>
<section class="jouer col-md-12">
 <div class="bouton1 col-md-10">
  <h2>
   <div class="link-one">
    <p>
     Le gagnant est
     <!-- Identifiant joueur gagnant -->
     <strong>{{ gagnant.pseudo }}</strong>,

     <!-- Score + faveur -->
     avec un score de {{ scoreG }} points, en gagnent les faveurs de {{ objectifsG }} personnages face à son terrible adversaire

     <!-- Identifiant joueur perdant -->
     <strong>{{ perdant.pseudo }}</strong>

     <!-- Score + faveur -->
     qui termine avec un score de {{ scoreP }} en gagnent les faveurs
     de {{ objectifsP }} personnages.
    </p>

    <a href="{{ path('index') }}">
     Retour à l'accueil
    </a>
   </div>
  </h2>
 </div>

</section>
{% endblock %}